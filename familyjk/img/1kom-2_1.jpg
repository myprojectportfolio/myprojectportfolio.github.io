š¡F{{ header }}{{ column_left }} 
<div id="content">
<div class="page-header">
<div class="container-fluid">
<div class="pull-right">
<a href="{{ store_review }}" data-toggle="tooltip" title="{{ button_store_review }}" class="btn btn-primary"><i class="fa fa-bullhorn"></i>&nbsp;&nbsp;{{ text_store_review }}</a>
<button type="submit" form="form-aridius_store_review" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
<h1>{{ heading_title }}</h1>
<ul class="breadcrumb">
{% for breadcrumb in breadcrumbs %} 
	<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
	{% endfor %} 
</ul>
</div>
</div>
<div class="container-fluid">
{% if (error_warning) %} 
	<div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }} 
	<button type="button" class="close" data-dismiss="alert">&times;</button>
	</div>
	{% endif %} 
<div class="panel panel-default">
<div class="panel-heading">
<h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
</div>
<div class="panel-body">
<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-aridius_store_review" class="form-horizontal">
<div class="form-group">
<label class="col-sm-3 control-label" >{{ entry_review_guest }}</label>
<div class="col-sm-9">
<label class="radio-inline">
{% if (module_aridius_store_review_review_guest) %} 
	<input type="radio" name="module_aridius_store_review_review_guest" value="0" />
	{{ text_yes }} 
	{% else %} 
	<input type="radio" name="module_aridius_store_review_review_guest" value="0" checked="checked" />
	{{ text_yes }} 
	{% endif %} 
</label>
<label class="radio-inline">
{% if (not module_aridius_store_review_review_guest) %} 
	<input type="radio" name="module_aridius_store_review_review_guest" value="1" />
	{{ text_no }} 
	{% else %} 
	<input type="radio" name="module_aridius_store_review_review_guest" value="1" checked="checked" />
	{{ text_no }} 
	{% endif %} 
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-3 control-label" >{{ entry_answer_button }}</label>
<div class="col-sm-9">
<label class="radio-inline">
{% if (module_aridius_store_review_display_answer_button) %} 
	<input type="radio" name="module_aridius_store_review_display_answer_button" value="0" />
	{{ text_yes }} 
	{% else %} 
	<input type="radio" name="module_aridius_store_review_display_answer_button" value="0" checked="checked" />
	{{ text_yes }} 
	{% endif %} 
</label>
