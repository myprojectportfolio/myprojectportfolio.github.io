{{ header }}{{ column_left }}
<div id="content">
<div class="page-header">
<div class="container-fluid">
<div class="pull-right">
<button type="submit" form="form-special" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i>&nbsp;&nbsp;{{ button_save }}</button>
<a onclick="apply();" class="btn btn-primary" data-toggle="tooltip" title="{{ button_apply }}" ><i class="fa fa-check"></i>&nbsp;&nbsp;{{ button_apply }}</a>
<a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
<h1>{{ heading_title }}</h1>
<ul class="breadcrumb">
{% for breadcrumb in breadcrumbs %}
<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
{% endfor %}
</ul>
</div>
</div>
<div class="container-fluid">
{% if (error_warning) %}
<div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
<button type="button" class="close" data-dismiss="alert">&times;</button>
</div>
{% endif %}
<div class="panel panel-default">
<div class="panel-heading">
<h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
</div>
<div class="panel-body">
<form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-special" class="form-horizontal">
<div class="form-group">
<label class="col-sm-2 control-label" for="input-name">{{ entry_name }}</label>
<div class="col-sm-10">
<input type="text" name="name" value="{{ name }}" placeholder="{{ entry_name }}" id="input-name" class="form-control" />
{% if (error_name) %}
<div class="text-danger">{{ error_name }}</div>
{% endif %}
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-limit">{{ entry_limit }}</label>
<div class="col-sm-10">
<input type="text" name="limit" value="{{ limit }}" placeholder="{{ entry_limit }}" id="input-limit" class="form-control" />
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-width">{{ entry_width }}</label>
<div class="col-sm-10">
<input type="text" name="width" value="{{ width }}" placeholder="{{ entry_width }}" id="input-width" class="form-control" />
{% if (error_width) %}
<div class="text-danger">{{ error_width }}</div>
{% endif %}
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-height">{{ entry_height }}</label>
<div class="col-sm-10">
<input type="text" name="height" value="{{ height }}" placeholder="{{ entry_height }}" id="input-height" class="form-control" />
{% if (error_height) %}
<div class="text-danger">{{ error_height }}</div>
{% endif %}
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-autoplay">{{ entry_autoplay }}</label>
<div class="col-sm-10">
<input type="text" name="autoplay" value="{{ autoplay }}" placeholder="{{ entry_autoplay }}" id="input-autoplay" class="form-control" />
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-items">{{ entry_items }}</label>
<div class="col-sm-10">
<input type="text" name="items" value="{{ items }}" placeholder="{{ entry_items }}" id="input-items" class="form-control" />
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-pag_speed">{{ entry_pag_speed }}</label>
<div class="col-sm-10">
<input type="text" name="pag_speed" value="{{ pag_speed }}" placeholder="{{ entry_pag_speed }}" id="input-pag_speed" class="form-control" />
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-rew_speed">{{ entry_rew_speed }}</label>
<div class="col-sm-10">
<input type="text" name="rew_speed" value="{{ rew_speed }}" placeholder="{{ entry_rew_speed }}" id="input-rew_speed" class="form-control" />
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-stophover">{{ entry_stophover }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (stophover) %}
<input type="radio" name="stophover" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="stophover" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not stophover) %}
<input type="radio" name="stophover" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="stophover" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-pagination">{{ entry_pagination }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (pagination) %}
<input type="radio" name="pagination" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="pagination" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not pagination) %}
<input type="radio" name="pagination" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="pagination" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-navigation">{{ entry_navigation }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (navigation) %}
<input type="radio" name="navigation" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="navigation" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not navigation) %}
<input type="radio" name="navigation" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="navigation" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<hr><hr>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-rat">{{ entry_rat }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (rat) %}
<input type="radio" name="rat" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="rat" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not rat) %}
<input type="radio" name="rat" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="rat" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-wish">{{ entry_wish }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (wish) %}
<input type="radio" name="wish" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="wish" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not wish) %}
<input type="radio" name="wish" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="wish" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-comp">{{ entry_comp }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (comp) %}
<input type="radio" name="comp" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="comp" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not comp) %}
<input type="radio" name="comp" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="comp" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-quickview">{{ entry_quickview }}</label>
<div class="col-sm-10">
<label class="radio-inline">
{% if (quickview) %}
<input type="radio" name="quickview" value="1" checked="checked" />
{{ text_yes }}
{% else %}
<input type="radio" name="quickview" value="1" />
{{ text_yes }}
{% endif %}
</label>
<label class="radio-inline">
{% if (not quickview) %}
<input type="radio" name="quickview" value="0" checked="checked" />
{{ text_no }}
{% else %}
<input type="radio" name="quickview" value="0" />
{{ text_no }}
{% endif %}
</label>
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label" for="input-status">{{ entry_status }}</label>
<div class="col-sm-10">
<select name="status" id="input-status" class="form-control">
{% if (status) %}
<option value="1" selected="selected">{{ text_enabled }}</option>
<option value="0">{{ text_disabled }}</option>
{% else %}
<option value="1">{{ text_enabled }}</option>
<option value="0" selected="selected">{{ text_disabled }}</option>
{% endif %}
</select>
</div>
</form>
</div>
</div>
</div>
</div>
<script><!--
function apply() {
	$('#form-special').append('<input type="hidden" id="apply" name="apply" value="1" />');
	$('#form-special').submit();
}
//--></script>
