				$data['header'] = $this->load->controller('common/header');

				$this->response->setOutput($this->load->view('error/not_found', $data));

			}
		}
	}

	public function review() {

		$this->load->language('extension/module/aridius_review_news');

		$this->load->model('extension/module/aridiusreviewnews');

		$data['text_no_reviews'] = $this->language->get('text_no_reviews');
		$data['text_write_reply'] = $this->language->get('text_write_reply');

		$data['aridius_news_display_answer_button'] = $this->config->get('module_aridius_news_display_answer_button');
		$limit = $this->config->get('module_aridius_news_limite_page');

		if (isset($this->request->get['page'])) {
			$page = $this->request->get['page'];
		} else {
			$page = 1;
		}

		$data['reviews'] = array();

		$review_total = $this->model_extension_module_aridiusreviewnews->getTotalReviewsByNewsId($this->request->get['aridius_news_id']);

		$results = $this->model_extension_module_aridiusreviewnews->getReviewsByNewsId($this->request->get['aridius_news_id'], ($page - 1) * $limit, $limit);

		foreach ($results as $result) {

			$parent = $this->model_extension_module_aridiusreviewnews->getReviewsByNewsId($this->request->get['aridius_news_id'],0,10,$result['review_id']);
			$parent_reviews=array();

			foreach($parent as $parents){
				$parent_reviews[] = array(
				'author'     => $parents['author'],
				'review_id'  => $parents['review_id'],
				'text'       => nl2br($parents['text']),
				'date_added' => date($this->language->get('date_format_short'), strtotime($parents['date_added']))
				);
			}

			$data['reviews'][] = array(
			'author'     => $result['author'],
			'parent_id'  => $parent_reviews,
			'review_id'  => $result['review_id'],
			'text'       => nl2br($result['text']),
			'date_added' => date($this->language->get('date_format_short'), strtotime($result['date_added']))
			);
		}

		$pagination = new Pagination();
		$pagination->total = $review_total;
		$pagination->page = $page;
		$pagination->limit = $limit;
		$pagination->url = $this->url->link('extension/module/aridius_news_layout/review', 'aridius_news_id=' . $this->request->get['aridius_news_id'] . '&page={page}');

		$data['pagination'] = $pagination->render();

		$data['results'] = sprintf($this->language->get('text_pagination'), ($review_total) ? (($page - 1) * $limit) + 1 : 0, ((($page - 1) * $limit) > ($review_total - $limit)) ? $review_total : ((($page - 1) * $limit) + $limit), $review_total, ceil($review_total / $limit));


		$this->response->setOutput($this->load->view('extension/module/aridius_review_news', $data));
	}

	public function reply(){

		$json=array();

		$this->load->language('extension/module/aridius_review_news');

		$data['parent']=(int)$this->request->post['parent'];

		// Captcha
		if ($this->config->get('captcha_' . $this->config->get('config_captcha') . '_status') && ($this->config->get('module_aridius_news_display_captcha')!=1)) {
			$captcha = $this->load->controller('extension/captcha/' . $this->config->get('config_captcha') . '/validate');

			if ($captcha) {
				$json['error'] = $captcha;
			}
		}

		if (($this->config->get('module_aridius_news_review_guest') !=1) || $this->customer->isLogged()) {
			$data['review_guest'] = true;
		} else {
			$data['review_guest'] = false;
		}

		if ($this->customer->isLogged()) {
			$data['customer_name'] = $this->customer->getFirstName() . '&nbsp;' . $this->customer->getLastName();
		} else {
			$data['customer_name'] = '';
		}

		$json['html'] = ($this->load->view('extension/module/aridius_review_news_form', $data));

		$this->response->addHeader('Content-Type: application/json');
		$this->response->setOutput(json_encode($json));
	}


	public function write() {

		$this->load->language('extension/module/aridius_review_news');

		$json = array();

		if ($this->request->server['REQUEST_METHOD'] == 'POST') {

			if ((utf8_strlen($this->request->post['name']) < 3) || (utf8_strlen($this->request->post['name']) > 25)) {
				$json['error'] = $this->language->get('error_name');
			}

			if ((utf8_strlen($this->request->post['text']) < 25) || (utf8_strlen($this->request->post['text']) > 1000)) {
				$json['error'] = $this->language->get('error_text');
			}

			// Captcha
			if ($this->config->get('captcha_' . $this->config->get('config_captcha') . '_status') && ($this->config->get('module_aridius_news_display_captcha')!=1)) {
				$captcha = $this->load->controller('extension/captcha/' . $this->config->get('config_captcha') . '/validate');

				if ($captcha) {
					$json['error'] = $captcha;
				}
			}

			if (!isset($json['error'])) {

				$this->load->model('extension/module/aridiusreviewnews');

				$this->model_extension_module_aridiusreviewnews->addReviewNews($this->request->get['aridius_news_id'], $this->request->post);

				$json['success'] = $this->language->get('text_success');
			}
		}

		$this->response->addHeader('Content-Type: application/json');
		$this->response->setOutput(json_encode($json));
	}
}
